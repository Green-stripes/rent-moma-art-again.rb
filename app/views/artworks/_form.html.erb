<%= simple_form_for artwork, data: {insert_in_list_target:"form", action:"submit->insert-in-list#adder"} do |f| %>
  <%= f.input :name %>
  <%= f.input :artist %>
  <%= f.input :cost_per_day %>
  <%= f.input :culture %>
  <%= f.input :completion_date %>
  <%= f.input :medium %>
  <%= f.input :image, as: :file%>
  <%= f.submit %>
<% end %>
